{
  "name": "@techteamer/docker-manager-library",
  "version": "1.0.0",
  "description": "A FaceKom Docker Manager Library",
  "main": "./build/docker-manager-library.js",
  "type": "module",
  "module": "./build/docker-manager-library.js",
  "types": "./build/docker-manager-library.d.ts",
  "exports": {
    ".": {
      "import": "./build/docker-manager-library.js",
      "require": "./build/docker-manager-library.cjs",
      "types": "./build/docker-manager-library.d.ts",
      "default": "./build/docker-manager-library.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "lint": "biome lint",
    "format": "biome format --write",
    "check": "biome check  --write",
    "prepare": "husky",
    "commit": "cz"
  },
  "engines": {
    "node": ">=20"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "files": [
    "build"
  ],
  "keywords": [
    "docker",
    "docker-library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TechTeamer/docker-manager-library.git"
  },
  "bugs": {
    "url": "https://github.com/TechTeamer/docker-manager-library/issues"
  },
  "homepage": "https://github.com/TechTeamer/docker-manager-library",
  "author": "TechTeamer",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.4",
    "husky": "^9.1.4",
    "semantic-release": "^24.0.0",
    "tsup": "^8.2.3",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitest": "^2.0.4"
  },
  "dependencies": {
    "dockerode": "^4.0.2",
    "dockerode-compose": "^1.4.0",
    "@types/dockerode": "^3.3.31",
    "@types/dockerode-compose": "^1.4.1",
    "js-yaml": "^4.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
